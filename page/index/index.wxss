}

.iconfont {
  font-family:"iconfont" !important;
  font-size:18px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-jiantouyou:before { content: "\e62d"; }
.month-panel{
  margin-left:10px;
  margin-right:10px;
  /* border:1px solid; */
  box-sizing:border-box;
  width: calc(100% - 20px);
  height: calc(100% - 40px);
  overflow: hidden;
}